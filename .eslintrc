{
    "extends": [
      "standard" 
    ],
    "parserOptions": {
       "sourceType": "module",
       "ecmaFeatures": {
          "jsx": true
       }
    },
    "globals": {
       "React": false,
       "File": false,
       "alert": false,
       "dispatch": false,
       "HTMLInputElement": false,
       "HTMLDivElement": false,
       "FB": false,
       "opener": false,
       "screen": false,
       "FormData": false,
       "Image": false,
       "URL": false,
       "expect": false,
       "test": false,
       "jest": false,
       "describe": false,
       "MediaRecorder": false,
       "Blob": false,
       "safari": false,
       "TEST_PROJECT_DATA_API_PATH": false,
       "TEST_USER_MEDIA_UPLOAD": false,
       "TEST_PROJECT_DATA_API_PATH_V2": false
    },
    "rules": {
      "arrow-parens": 2,
      "curly": 0,
      "comma-dangle": [
        2,
        "never"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "react/display-name": "off",
      "react/prop-types": "off",
      "no-sequences": 0,
      "camelcase": 0,
      "no-proto": 0,
      "no-throw-literal": 0,
      "no-useless-escape": 0,
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "key-spacing": 0,
      "no-underscore-dangle": 0,
      "no-unused-expressions": 0,
      "space-before-function-paren": 2,
      "no-unused-vars": "warn",
      "no-shadow": 0,
      "no-shadow-restricted-names": 0,
      "no-use-before-define": [
        "error",
        {
          "functions": true,
          "classes": true
        }
      ],
      "no-extend-native": 0,
      "no-var": 2,
      "new-cap": 0,
      "quotes": 2,
      "semi-spacing": 0,
      "space-unary-ops": 0,
      "space-infix-ops": 0,
      "consistent-return": 0,
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "strict": 0
    }
  }
  